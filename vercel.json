{
    "version": 2,
    "builds": [
        {
            "src": "app/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "app/static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "app/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "app/main.py"
        }
    ],
    "env": {
        "PYTHON_VERSION": "3.9"
    }
} 